@model SisRestaurante.Entity.Restaurante

@{
    ViewBag.Title = "Detalhes do Restaurante";
}

<h2>Detalhes do Restaurante</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Nome)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nome)
        </dd>

    </dl>
</div>
<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        @Html.ActionLink("Cancelar", "Index", new { @area = "" }, new { @class = "btn btn-danger" })
        @Html.ActionLink("Alterar", "Edit", new { id = Model.RestauranteId }, new { @class = "btn btn-default" })
    </div>
</div>

